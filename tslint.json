{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "eofline": true,
    "indent": [true, "spaces", 2],
    "interface-name": false,
    "ordered-imports": true,
    "no-any": true,
    "no-console": false,
    "no-shadowed-variable": false,
    "quotemark": [true, "single"],
    "prefer-const": true,
    "trailing-comma": true
  }
}
